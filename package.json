{
  "name": "release-it-demo",
  "version": "3.1.7",
  "scripts": {
    "release-setup": "release-it --github.release=false && echo \"Pull Request the release changes back to main branch to initiate the release process\"",
    "release-process-ci": "dotenv -- release-it --no-increment --ci --verbose --no-git --github.release=true --github.preRelease=true --no-plugins.@release-it/conventional-changelog.infile",
    "release-finalise-ci": "dotenv -- release-it --no-increment --ci --verbose --no-git --github.release=true --github.preRelease=false --no-plugins.@release-it/conventional-changelog.infile"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.1",
    "dotenv-cli": "^10.0.0",
    "release-it": "^19.0.4"
  }
}
